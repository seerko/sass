                  <a href='{{link}}' class='product__link'>{{text}}</a>
              {{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
  </div>

	<footer class="footer">
		{{#each footer}}
			<div class="footer__content">
				<strong class="footer__title">
					{{title}}
				</strong>
				<div class="footer__links">
					{{#each links}}
						<a href="{{url}}" class="footer__links-item">{{text}}</a>
					{{/each}}
				</div>
			</div>
		{{/each}}
	</footer>
</div>