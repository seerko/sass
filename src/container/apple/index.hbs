                  <a href='{{link}}' class='product__link'>{{text}}</a>
              {{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
  </div>
	<div class='section__double'>
    <div>
      {{#with gift}}
        <section
          class='product product--small product--gift product--light product--top'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
            <p class='product__description'>{{descriptions}}</p>
            <div class='product__action'>
              {{#with link}}
									<a href='{{link}}' class='product__link'>{{text}}</a>
							{{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
		<div>
      {{#with card}}
        <section
          class='product product--small product--card product--light product--top'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
            <p class='product__description'>{{descriptions}}</p>
            <div class='product__action'>
