<div class='page'>
  {{#with header}}
    <div class='header'>
      <header class='header__content'>
        <div class='header__block'>
          <img src='/svg/apple.svg' alt='Apple' class='header__icon' />
          <nav class='header__block'>
            {{#each links}}
              <a href='{{url}}' class='header__link'>{{text}}</a>
            {{/each}}
          </nav>
        </div>
        <div class='header__block'>
          <img src='/svg/search.svg' alt='Search' class='header__icon' />
          <img src='/svg/profile.svg' alt='Profile' class='header__icon' />
        </div>
      </header>

    </div>
    {{#with ad}}
      <div class='ad'>
				<div class="ad__content">
					<h3 class='ad__title'>Get $200–$600 in credit toward iPhone 14
						or iPhone 14 Pro when you trade in iPhone 11 or higher.</h3>
					<a href='{{link.url}}' class='ad__link'>{{link.text}}</a>
				</div>
      </div>
    {{/with}}
  {{/with}}
  {{#with iphone_pro}}
    <section
      class='product product--big product--iphone-pro product--dark product--top'
    >
      <div class='product__content'>
        <h1 class='product__title'>{{title}}</h1>
        <p class='product__description'>{{descriptions}}</p>
        <div class='product__action'>
          {{#with link}}
            {{#each text}}
              <a href='{{../link}}' class='product__link'>{{this}}</a>
            {{/each}}
          {{/with}}
        </div>
      </div>
    </section>
  {{/with}}

  {{#with iphone}}
    <section
      class='product product--big product--iphone product--light product--top'
    >
      <div class='product__content'>
        <h1 class='product__title'>{{title}}</h1>
        <p class='product__description'>{{descriptions}}</p>
        <div class='product__action'>
          {{#with link}}
            {{#each text}}
              <a href='{{../link}}' class='product__link'>{{this}}</a>
            {{/each}}
          {{/with}}
        </div>
      </div>
    </section>
  {{/with}}

	{{#with iwatch}}
    <section
      class='product product--big product--iwatch product--dark product--top'
    >
      <div class='product__content'>
        <h1 class='product__title'>{{title}}</h1>
        <p class='product__description'>{{descriptions}}</p>
        <div class='product__action'>
          {{#with link}}
            {{#each text}}
              <a href='{{../link}}' class='product__link'>{{this}}</a>
            {{/each}}
          {{/with}}
        </div>
      </div>
    </section>
  {{/with}}

  <div class='section__double'>
    <div>
      {{#with trade_in}}
        <section
          class='product product--small product--trade_in product--light product--top'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
            <p class='product__description'>{{descriptions}}</p>
            <div class='product__action'>
              {{#with link}}
                  <a href='{{link}}' class='product__link'>{{text}}</a>
              {{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
		<div>
      {{#with conference}}
        <section
          class='product product--small product--conference product--dark product--bottom'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
            <p class='product__description'>{{descriptions}}</p>
            <div class='product__action'>
              {{#with link}}
                  <a href='{{link}}' class='product__link'>{{text}}</a>
              {{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
  </div>
	<div class='section__double'>
    <div>
      {{#with ipad}}
        <section
          class='product product--small product--ipad product--light product--top'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
            <p class='product__description'>{{descriptions}}</p>
            <div class='product__action'>
              {{#with link}}
								{{#each text}}
									<a href='{{../link}}' class='product__link'>{{this}}</a>
								{{/each}}
							{{/with}}
            </div>
          </div>
        </section>
      {{/with}}
    </div>
		<div>
      {{#with small_business}}
        <section
          class='product product--small product--small_business product--dark product--top'
        >
          <div class='product__content'>
            <h1 class='product__title'>{{title}}</h1>
